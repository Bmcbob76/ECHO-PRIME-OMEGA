        "enabled": true,
        "auto_heal": true,
        "pattern_matching": true
      }
    },
    "logging": {
      "level": "INFO",
      "file": "sentinel.log",
      "console": true
    },
    "mls_integration": {
      "enabled": true,
      "registry_path": "P:\\ECHO_PRIME\\MLS_CLEAN\\PRODUCTION\\mls_registry.json"
    }
  }
}
