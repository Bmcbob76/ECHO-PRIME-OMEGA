{
  "phoenix_sentinel": {
    "version": "1.0.0",
    "authority_level": 11.0,
    "components": {
      "windows_api": {
        "enabled": true,
        "debug_privileges": true,
        "injection_allowed": false
      },
      "autonomous_cpu": {
        "enabled": true,
        "cpu_threshold": 80,
